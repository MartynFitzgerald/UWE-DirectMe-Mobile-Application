{
  "expo": {
    "name": "DirectMe",
    "slug": "DirectMe-Mobile-Prototype",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "hidden": false, 
      "translucent": true,
      "barStyle": "light-content",
      "backgroundColor": "#00000000"
    },
    "ios": {
      "supportsTablet": false
    }
  }
}
